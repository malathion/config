! X cursor
Xcursor.theme: Vanilla-DMZ
! Xcursor.size: 16

*urlLauncher: /usr/bin/xdg-open

! XFT and Font settings
Xft*dpi:       96
Xft*antialias: true
Xft*hinting:   true
Xft*hintstyle: hintfull
Xft*rgba:      rgb
*font: xft:Inconsolata\-g:style=g:pixelsize=20,xft:Consolas:style=Regular:pixelsize=20,xft:FreeMono:style=Bold:pixelsize=20,FreeMono:style=Regular:pixelsize=20,xft:Courier New:pixelsize=20

*cursorColor: #bfbfbf
*colorUL: #c5f779
*underlineColor: #c5f779
*background: #101010
*foreground: #f2f2f2
! color0    black
*color0: #3f3f3f
! color8    light black
*color8: #709080
! color1    red
*color1: #705050
! color9    light red
*color9: #cc9393
! color2    green
*color2: #60b48a
! color10   light green
*color10: #7f9f7f
! color3    yellow
*color3: #dfaf8f
! color11   light yellow
*color11: #f0dfaf
! color4    blue
*color4: #3399CC
! color12   light blue
*color12: #91C9E8
! color5    purple
*color5: #dc8cc3
! color13   light purple
*color13: #ec93d3
! color6    cyan
*color6: #8cd0d3
! color14   light cyan
*color14: #93e0e3
! color7    white
*color7: #DCDCCC
! color15:  light white
*color15: #ffffff

! Urxvt
! Locale
URxvt.imLocale: en_US.UTF-8
! disable the urxvt unicode input mechanism (I use a different one)
URxvt.iso14755: false
! URxvt.geometry: 80x25
URxvt*inheritPixmap: true
URxvt.internalBorder: 0
URxvt.jumpScroll: true
! URxvt.loginShell: true
URxvt.perl-ext-common: default,matcher,searchable-scrollback
URxvt.pointerBlank: true
URxvt.saveLines: 8192
URxvt.secondaryScroll: true
URxvt.scrollBar: false
URxvt.scrollTtyKeypress: true
URxvt.scrollWithBuffer: true
URxvt.visualBell: true
URxvt.insecure: false

