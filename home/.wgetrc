#wget --append-output=~/wget.log --tries=10 --waitretry=5 --no-verbose --continue --no-directories --no-cache --html-extension --ignore-length --no-parent http://files.slashquit.net/x/q3/pak0.pk3

#equivalent to --progress=string
progress = bar

#logfile = ~/logs/wget.log

#same as --ignore-case
ignore_case = on

#see --cookies
cookies = on

#don't change timestamps (so we can use noclobber later)
timestamping = off

#see --no-use-server-timestamps
use_server_timestamps = on

#same as -t 'n'
tries = 10

#same as -w 'n'
wait = 10

#same as --waitretry = 'n'
wait_retry = 5
